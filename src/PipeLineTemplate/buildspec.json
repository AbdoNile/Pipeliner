{
    "version": "0.2",
    "phases": {
      "install": {
        "runtime-versions": {
          "nodejs": 10
        }
      },
      "pre_build": {
        "commands": ["cd src/PipeLineTemplate", "npm install"],
      },
      "build": {
        "commands": [
          "npm run build", 
        
          "npm run cdk diff"
        ]
      }
    },
    "cache": {
      "paths": ["node_modules/**/*"]
  }
}
  